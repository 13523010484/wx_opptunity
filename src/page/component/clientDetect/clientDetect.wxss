page {
    background-color: #f5f5f5;
}

.title {
    font-size: 30rpx;
    padding: 24rpx;
    color: #808080;
}

.page_input {
    padding: 24rpx;
    background-color: #fff;
    display: flex;
    height: 90rpx;
    line-height: 90rpx;
    align-items: center;
    font-size: 28rpx;
}

.page_input>view:first-child {
    width: 120rpx;
}

.page_input>view:first-child {
    width: 120rpx;
}

.page_input>view:nth-child(3) {
    width: 90rpx;
}

.page_input>input {
    width: 150rpx;
    margin-right: 24rpx;
    color: #666;
    border-bottom: 1rpx #eee solid;
}

.input_btn {
    width: 120rpx;
    height: 60rpx;
    line-height: 60rpx;
    text-align: center;
    border-radius: 60rpx;
    color: #fff;
    background-color: #4a90e2;
}

.icon_warn, .icon_success {
    padding: 0 24rpx;
    font-size: 32rpx;
    height: 90rpx;
    display: flex;
    align-items: center;
}

.icon_warn icon, .icon_success icon {
    margin-right: 14rpx;
}

.icon_warn {
    color: #e64340;
}

.icon_success {
    color: #09bb07;
}
