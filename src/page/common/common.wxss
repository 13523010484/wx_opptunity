.item {
    color: #666;
    padding: 0 24rpx;
    font-size: 28rpx;
    height: 90rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
    border-bottom: 1rpx #eee solid;
    background-color: #fff;
}

.load_more, .empty {
    font-size: 24rpx;
    color: #999;
    text-align: center;
    height: 80rpx;
    line-height: 80rpx;
}

.empty {
    background-color: #f5f5f5;
}
